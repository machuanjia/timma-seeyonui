                                        }
                                    }
                                    return true;
                                }
                                function beforeDrop5(treeId, treeNodes, targetNode, moveType) {
                                    return targetNode ? targetNode.drop !== false : true;
                                }

                                $(document).ready(function () {
                                    $.fn.zTree.init($("#treeDemo51"), setting5, zNodes5);
                                    $.fn.zTree.init($("#treeDemo52"), setting5);

                                });
                            &lt;/script>
                        </pre>
                    </div>
