                    <div class="code_box">
                        <pre class="brush:html;">
                             &lt;div id="toolbar">&lt;/div>
                        </pre>
                        <pre class="brush:js;">
                            <script type="text/javascript">
                                 function getValue(){
                                    var index=$("#select")[0].selectedIndex;
                                    alert($("#select")[0][index].innerHTML);
                                 }

                                $().ready(function(){
                                    var tt = $("#toolbar").toolbar({
                                        //searchHtml:"search",
                                        toolbar: [{
                                            
                                            id: "transmit",
                                            name: "转发",
                                            className: "ico16",
                                            click:function(){
                                                    alert("转发");
                                                },
                                            subMenu: [{
                                                id:'sssssssssss',
                                                name: "协同",
                                                click: function(){
                                                    alert(111);
                                                }
