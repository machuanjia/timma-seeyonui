    <script>
        $(function () {
            SyntaxHighlighter.all();
            $("pre").each(function (i) {
                $("pre").eq(i).html(function () {
                    return $("pre").eq(i).html().replace(/</g, "&lt;");
                })
            })

            var WrapTopBar_data_json = [
                {
                    name: "HTML5标签",
                    data: [
                        { name: "video", href: "tag_video.html" },
                        { name: "video", href: "tag_video.html" },
