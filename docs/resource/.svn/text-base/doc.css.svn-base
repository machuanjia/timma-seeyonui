
.display_block{
	display:block;
}
.clear{
	clear:both;
}
.float_left{
	float:left;
}
.aline-left{
	text-align:left;
}
.aline-center{
	text-align:center;
}
.aline-right{
	text-align:right;
}
.header{
	background-color:#1462ba;
	text-align:left;
	padding:0 10px;
	font-size:20px;
	color:#ffffff;
	font-weight:bold;
}

.main-bg{
	background:#ececec;
}
.padding5{
	padding:5px;
}
.body-pading{
	padding:40px;
}
.classification{
	
	font-size:12px;
}

	
.title{
	color:#2b5a90;
	font-size:18px;
	
	margin-bottom:2px;
	font-weight:bold;
}
.code_list{border:1px solid #ccc;padding:5px;}
.code_list,.list{
	margin:10px 0px;
}
.view_source{font-weight:bold;font-size:14px;margin-top:10px;}
.view_source_hidden{background-position:left top;}
.code{
	clear:both;
	padding:10px;

	border:1px #785 dotted;
	margin:10px 0px;
}
.ue{
	clear:left;
	margin-top:10px;
	margin-bottom:10px;
	padding:10px;
	background:#ececec;
	border:1px #785 dotted;
}
.propoty{
	margin-top:10px;
	margin-bottom:10px;
	padding:10px;
	background:#ececec;
	border:1px #785 dotted;
}
.showList{
	border-collapse:collapse;
}
.showList .head{
}
.showList .head th{
	background:#ececec;
	padding:5px;
	border:1px #d2d2d2 solid;
	font-weight:bold;
	font-size:14px;
}
.showList tbody td{
	padding:5px;
	border:1px #d2d2d2 solid;
	border-collapse:collapse;
}
.showList tbody td div{background:#e8f8ff;border:1px solid #ccc;padding:5px;}
.showDemo{
	color:blue;
	margin:0px 5px;
	border:1px #d1d1d1 solid;
	height:10px;
	width:10px;
	line-height:10px;
	vertical-align:middle;
	overflow:hidden;
	cursor:pointer;
	background:#d7d7d7;
	display:inline-block;
	text-align:center;
}
.hidden{
	display:none;
}
.show{
	display:block;
}
.table-row{
	display:table-row;
}
















